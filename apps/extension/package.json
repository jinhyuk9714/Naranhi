{
  "name": "@naranhi/extension",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt --browser firefox",
    "build": "wxt build",
    "build:firefox": "wxt build --browser firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip --browser firefox",
    "clean": "rm -rf .output .wxt",
    "postinstall": "wxt prepare",
    "test": "vitest run --passWithNoTests",
    "lint": "wxt build"
  },
  "dependencies": {
    "@naranhi/core": "workspace:*",
    "@naranhi/engines": "workspace:*",
    "@naranhi/ui": "workspace:*",
    "@naranhi/youtube": "workspace:*",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@wxt-dev/module-react": "^1.1.0",
    "autoprefixer": "^10.4.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0",
    "wxt": "^0.20.0"
  }
}
